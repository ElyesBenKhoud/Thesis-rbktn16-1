{
  "expo": {
    "name": "bycycle",
    "slug": "bycycle",
    "version": "1.0.0",
    "orientation": "portrait",

    "icon": "./assets/LogoBike.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "name.name"

    },
    "android": {
      "package": "com.bycycle.bycycle",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "build": {
        "babel": {
          "include": [
            "react-router-native", "deprecated-react-native-listview"

          ]
        }
      }
    }
  }
}
